@model IEnumerable<Admin28.Models.JC_HC_USERS>

@{
    ViewBag.Title = "Subscriber Index";
}

<h2>Subscriber Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")

    @using (Html.BeginForm())
    {
        <p>User Last Name: @Html.TextBox("SearchString") <br />
        <input type="submit" class="btn btn-primary" value="Filter" /></p>
    }
</p>
<table class="table table-bordered table-condensed table-striped" id="paged">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LNAME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FNAME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AG_ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EMAIL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OOF)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LEO)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LNAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FNAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AG_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EMAIL)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OOF)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LEO)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.ID }, new { @class = "text-warning" }) |
                @Html.ActionLink("Details", "Details", new { id = item.ID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.ID }, new { @class = "text-danger" })
            </td>
        </tr>
    }

</table>
