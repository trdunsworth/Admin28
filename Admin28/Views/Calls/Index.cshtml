@model IEnumerable<Admin28.Models.JC_HC_CURENT>

@{
    ViewBag.Title = "Current Call List";
}

<h2>Current Call List</h2>

<table class="table table-bordered table-condensed table-striped" id="paged">
    <tr>
        <th>@Html.DisplayNameFor(model => model.EID)</th>
        <th>@Html.DisplayNameFor(model => model.NUM_1)</th>
        <th>@Html.DisplayNameFor(model => model.AG_ID)</th>
        <th>@Html.DisplayNameFor(model => model.TYCOD)</th>
        <th>@Html.DisplayNameFor(model => model.SUB_TYCOD)</th>
        <th>@Html.DisplayNameFor(model => model.UNIT_COUNT)</th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>@Html.DisplayFor(modelItem => item.EID)</td>
        <td>@Html.DisplayFor(modelItem => item.NUM_1)</td>
        <td>@Html.DisplayFor(modelItem => item.AG_ID)</td>
        <td>@Html.DisplayFor(modelItem => item.TYCOD)</td>
        <td>@Html.DisplayFor(modelItem => item.SUB_TYCOD)</td>
        <td>@Html.DisplayFor(modelItem => item.UNIT_COUNT)</td>
        <td>@Html.ActionLink("Details", "Details", new { eid = item.EID })</td>
    </tr>
}

</table>
