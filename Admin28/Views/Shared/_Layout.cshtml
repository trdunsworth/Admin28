<!DOCTYPE html>
<html>
<head lang="en">
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>JCSO Hot Calls Administration &ndash; @ViewBag.Title</title>
    <link rel="author" href="humans.txt" />
    <link rel="stylesheet" href="~/Content/bootstrap.min.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/bootstrap-theme.min.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/Site.css" type="text/css" />
    <script src="~/Scripts/modernizr-2.7.2.js"></script>
    <!-- HTML5 shim and repsond.js IE8 support of HTML5 elements & media queries -->
    <!--[if lt IE 9]>
        <script src="~/Scripts/html5shiv.js"></script>
    <![endif]-->
    <meta name="description" content="JCSO Hot Calls Administration Page" />
</head>
<body>
    <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <!-- Brand and toggle get grouped for better module display -->
        <div class="navbar-header">
            <button class="navbar-toggle" type="button" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle Navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            @Html.ActionLink("JCSO Hot Calls", "Index", "Home", null, new { @class = "navbar-brand" })
        </div>
        <div class="collapse navbar-collapse">
            <ul class="nav navbar-nav">
                <li>@Html.ActionLink("Users", "Index", "Users")</li>
                <li>@Html.ActionLink("Subscriptions", "Index", "Subscriptions")</li>
                <li>@Html.ActionLink("Call List", "Index", "Calls")</li>
                <li>@Html.ActionLink("Locations", "Index", "Locations")</li>
                <li>@Html.ActionLink("Types", "Index", "Types")</li>
                <li>@Html.ActionLink("Agencies", "Index", "Agency")</li>
                <li>@Html.ActionLink("Sent Calls", "Index", "Emails")</li>
            </ul>
        </div>
    </nav>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year &ndash; Johnson County Sheriff&rsquo;s Office, All rights reserved. Email the <a href="mailto:tony.dunsworth@jocogov.org" class="text-info">site administrator</a> for corrections or questions.</p>
        </footer>
    </div>
    <script src="~/Scripts/jquery-2.1.1.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/respond.min.js"></script>
    <script src="~/Scripts/respond.matchmedia.addListener.min.js"></script>
    @RenderSection("scripts", required: false)
    <script type="text/javascript" src="~/Scripts/oneSimpleTablePaging-1.0.js"></script>
    <script type="text/javascript">$('#paged').oneSimpleTablePagination({ rowsPerPage: 15 });</script>
</body>
</html>
